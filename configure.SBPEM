# Lib Path
NETCDFINC = -I$(NETCDF)/include
INCLUDE   = -I. $(NETCDFINC)

# GFORTRAN COMPLIER
F90     = gfortran
FCFLAGS = -ffixed-line-length-none -ffree-line-length-none #-fmax-array-constructor=655350 -fmax-stack-var-size=6553500 -fopenmp -fdefault-real-8
CFLAGS  =
OPT     = -O3 -floop-parallelize-all -ftree-parallelize-loops=4
AR      = ar
ARFLAGS = ru
MAKE    = make

EXENAME = SBPEM.exe
